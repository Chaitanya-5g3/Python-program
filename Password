password=input()
l=len(password)
isdigit=False
islower=False
isupper=False
isspecial=False
special_character=['@', '#','? ', 'â‚¹', '$']
for i in password:
  if l<8:
    print("invalid password ")
  elif i.isupper():
     isupper=True
  elif i.islower():
     islower=True
  elif i.isdigit():
     isdigit=True
  elif i in special_character:
      isspecial=True
if isupper==True and islower==True and isdigit==True and isspecial==True:
   print("Valid password ")
else:
   print("invalid password ")
